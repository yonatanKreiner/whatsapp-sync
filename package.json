{
  "name": "whatsapp-web-reveng",
  "version": "1.0.0",
  "description": "A graphical, web-based client for WhatsApp Web, using the reverse engineered Python code as backend.",
  "main": "index.js",
  "scripts": {
    "start:backend": "nodemon --exec python ./backend/whatsapp_web_backend.py -i client -e py",
    "start:server": "nodemon index.js -i client -e js",
    "start:dev": "concurrently --kill-others \"npm run start:server\" \"npm run start:backend\"",
    "start": "PORT=443 node index.js"
  },
  "author": "sigalor",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sigalor/whatsapp-web-reveng.git"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.2",
    "google-contacts-api-wrapper": "^1.0.4",
    "googleapis": "^32.0.0",
    "libphonenumber-js": "^1.2.15",
    "lodash": "^4.17.4",
    "path": "^0.12.7",
    "querystring": "^0.2.0",
    "string_decoder": "^1.0.3",
    "ws": "^3.3.2"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "nodemon": "^1.12.5"
  }
}
